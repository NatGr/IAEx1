<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### AUCTION AGENTS ### -->
	<agent name="auction-main-17">
		<set class-path="bin/"/>
		<set class-name="auction.AuctionAgent"/>
		
		<set search-algorithm="SimulatedAnnealing" />
		<set temperature-begin="1000."/>
		<set temperature-end="100." />
	</agent>
	
	<agent name="auction-opp-est">
		<set class-path="bin/"/>
		<set class-name="auction.AuctionAgent2"/>
		
		<set search-algorithm="SimulatedAnnealing" />
		<set temperature-begin="1000."/>
		<set temperature-end="100." />
	</agent>
	
	<agent name="auction-dummy">
		<set class-path="bin/"/>
		<set class-name="auction.AuctionDummy"/>
		
		<set search-algorithm="SimulatedAnnealing" />
		<set temperature-begin="1000."/>
		<set temperature-end="100." />
	</agent>
	
</agents>
